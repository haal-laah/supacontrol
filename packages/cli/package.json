{
  "name": "@supacontrol/cli",
  "version": "0.1.2",
  "description": "CLI wrapper around Supabase CLI with safety guards and environment management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "supacontrol": "dist/index.js",
    "supac": "dist/index.js",
    "spc": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "pretest": "node scripts/verify-fixtures.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "verify-fixtures": "node scripts/verify-fixtures.js"
  },
  "keywords": [
    "supabase",
    "cli",
    "database",
    "safety",
    "guards",
    "migrations",
    "environment",
    "production",
    "devops"
  ],
  "author": "SupaControl Contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haal-laah/supacontrol/issues"
  },
  "homepage": "https://github.com/haal-laah/supacontrol#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haal-laah/supacontrol.git",
    "directory": "packages/cli"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.8",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.2",
    "prettier": "^3.8.0",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "commander": "^14.0.2",
    "execa": "^9.6.1",
    "picocolors": "^1.1.1",
    "smol-toml": "^1.6.0",
    "zod": "^4.3.5"
  }
}
